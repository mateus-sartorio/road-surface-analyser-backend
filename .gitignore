# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore other database files.
/db/*.db

# Ignore all logfiles and temp files.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials/*.key

# Ignore node modules and Yarn integrity
/node_modules/
/yarn-error.log

# Ignore precompiled assets.
/public/assets
/public/packs
/public/packs-test
/public/packs-development
/public/packs-production
/public/*.js
/public/*.css
/public/*.map

# Ignore byebug command history file.
.byebug_history

# Ignore environment files
.env
.env.*

# Ignore system test screenshots.
/tmp/screenshots

# VS Code
.vscode/

# Ignore coverage reports generated by SimpleCov.
/coverage/

# Ignore Docker files (if using Docker)
/docker-compose.override.yml
/docker-compose.override.yaml

# macOS system files
.DS_Store

# Ignore spring server pid
/tmp/pids/server.pid

# Ignore pry history
.pry_history

# Ignore compiled cache files
/tmp/cache

# Ignore rails restart files
/tmp/restart.txt

# Ignore Redis dump files
*.rdb

# Ignore PostgreSQL and MySQL dumps
*.sql
*.dump

# Ignore Capybara artifacts
capybara-*.html

# Ignore RuboCop cache
.rubocop-*

# Ignore IDEs
.idea/
*.swp
*.swo

# Ignore test coverage
/coverage/